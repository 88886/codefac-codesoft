<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <action-listener>org.primefaces.application.DialogActionListener</action-listener>
        <navigation-handler>org.primefaces.application.DialogNavigationHandler</navigation-handler>
        <view-handler>org.primefaces.application.DialogViewHandler</view-handler>
    </application>
    
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>cliente</from-outcome>
            <to-view-id>/admin/cliente.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>buscarCliente</from-outcome>
            <to-view-id>/admin/buscarCliente.xhtml</to-view-id>
        </navigation-case>
        
        
        <navigation-case>
            <from-outcome>compra</from-outcome>
            <to-view-id>/operador/compra2.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>login.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>crearDistribuidor</from-outcome>
            <to-view-id>/operador/crearDistribuidor.xhtml</to-view-id>
        </navigation-case>
        
        
        <navigation-case>
            <from-outcome>factura</from-outcome>
            <to-view-id>/operador/factura.xhtml</to-view-id>
        </navigation-case>
        
        
        <navigation-case>
            <from-outcome>crearCliente</from-outcome>
            <to-view-id>/operador/nuevoCliente.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>crearCatalogoProducto</from-outcome>
            <to-view-id>/operador/crearCatalogoProducto.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>indexOperador</from-outcome>
            <to-view-id>/operador/index.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>reporteCliente</from-outcome>
            <to-view-id>/admin/reporteSimpleCliente.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>editarInventario</from-outcome>
            <to-view-id>/admin/editarInventarioCatalogo.xhtml</to-view-id>
        </navigation-case>
        
        
        <navigation-case>
            <from-outcome>productoGestion</from-outcome>
            <to-view-id>/admin/producto.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>proforma</from-outcome>
            <to-view-id>/operador/proforma.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>notaCredito</from-outcome>
            <to-view-id>/operador/notaCredito.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>pruebaConsulta</from-outcome>
            <to-view-id>/test/pruebaConsulta.xhtml</to-view-id>
        </navigation-case>

        
    </navigation-rule>
    
</faces-config>
